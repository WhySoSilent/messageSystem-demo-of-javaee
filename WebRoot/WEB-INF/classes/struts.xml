<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
    "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="controller" extends="struts-default">
		<action name="Register" class="controller.Register">
			<result name="success">/index.jsp</result>
			<result name="error">/register.jsp</result>
		</action>
		<action name="Login" class="controller.Login">
			<result name="success">/index.jsp</result>
			<result name="error">/login.jsp</result>
		</action>
		<action name="Logout" class="controller.Logout">
			<result name="success">/index.jsp</result>
		</action>
		<action name="new" class="controller.Pub">
			<result name="success">/index.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		<action name="index">
			<result>/index.jsp</result>
		</action>
		<action name="login">
			<result>/login.jsp</result>
		</action>
		<action name="register">
			<result>/register.jsp</result>
		</action>
		<!-- Add your actions here -->
	</package>
</struts>